/* pages/list-template/list-template.wxss */
.tmpContainer {
	display: flex;
	flex-direction: column;

}
.avatar_date {
	padding: 10rpx;
}

.avatar_date image {
	width: 60rpx;
	height: 60rpx;
	vertical-align: middle;
}

.company {
	font-size: 40rpx;
	font-weight: 700;
	margin-left: 10rpx;
}

.list_img {
	width: 100%;
	height: 460rpx;
	margin-top: 20rpx;
}

.content {
	font-size: 30rpx;
	text-indent: 30rpx;
}

.collection_love_container image {
	width: 32rpx;
	height: 32rpx;
	vertical-align: middle;
	margin-right: 10rpx;
}


.collection_love_container text {
	font-size: 28rpx;
	margin-right: 10rpx;
}

